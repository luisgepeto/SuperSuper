{
  "name": "supersuper",
  "version": "1.0.0",
  "description": "A web application that allows users to compare products while shopping in a supermarket",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "nodemon server/index.js",
    "build": "vite build",
    "build:backend": "tsc --project server/tsconfig.json",
    "preview": "vite preview",
    "start": "node server/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luisgepeto/SuperSuper.git"
  },
