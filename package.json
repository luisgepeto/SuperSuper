  },
  "homepage": "https://github.com/luisgepeto/SuperSuper#readme",
  "dependencies": {
    "@huggingface/transformers": "^3.8.1",
    "@yudiel/react-qr-scanner": "^2.0.8",
    "cors": "^2.8.5",
